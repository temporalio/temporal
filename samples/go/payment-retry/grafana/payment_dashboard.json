    {
      "type": "row",
      "title": "Time Series Graphs",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 27 },
      "id": 12,
      "panels": []
    },
    {
      "title": "Workflow Rates",
      "type": "timeseries",
      "datasource": "Prometheus",
      "id": 13,
      "gridPos": { "h": 9, "w": 12, "x": 0, "y": 28 },
      "targets": [
        {
          "expr": "sum(rate(payment_workflows_started_total[1m]))",
          "legendFormat": "Started",
          "refId": "A"
        },
        {
          "expr": "sum(rate(payment_workflows_completed_total{status=\"success\"}[1m]))",
          "legendFormat": "Succeeded",
          "refId": "B"
        },
        {
          "expr": "sum(rate(payment_workflows_failed_permanently_total[1m]))",
