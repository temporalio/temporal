      "version": "7.0.0"
    },
    {
      "type": "datasource",
      "id": "prometheus",
      "name": "Prometheus",
      "version": "1.0.0"
    }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "gnetId": null,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "panels": [
    {
      "type": "row",
      "title": "Workflow Overview",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 0 },
      "id": 1,
      "panels": []
    },
    {
      "title": "Workflows Started",
      "type": "stat",
      "datasource": "Prometheus",
      "id": 2,
      "gridPos": { "h": 8, "w": 6, "x": 0, "y": 1 },
      "targets": [
        {
          "expr": "sum(rate(payment_workflows_started_total[5m]))",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] },
          "unit": "ops"
        },
        "overrides": []
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "orientation": "auto", "textMode": "auto", "colorMode": "value", "graphMode": "area", "justifyMode": "auto" }
    },
    {
      "title": "Workflow Success Rate",
      "type": "stat",
      "datasource": "Prometheus",
      "id": 3,
      "gridPos": { "h": 8, "w": 6, "x": 6, "y": 1 },
      "targets": [
        {
          "expr": "sum(rate(payment_workflows_completed_total{status=\"success\"}[5m])) / sum(rate(payment_workflows_started_total[5m])) * 100",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": 80 },{ "color": "orange", "value": 60 }, { "color": "red", "value": null }] },
          "unit": "percent"
        },
        "overrides": []
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "orientation": "auto", "textMode": "auto", "colorMode": "value", "graphMode": "area", "justifyMode": "auto" }
    },
    {
      "title": "Workflows Succeeded (Total)",
      "type": "stat",
      "datasource": "Prometheus",
      "id": 4,
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 1 },
      "targets": [
        {
          "expr": "sum(increase(payment_workflows_completed_total{status=\"success\"}[$__range]))",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "short", "mappings": [], "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] } },
        "overrides": []
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "orientation": "auto", "textMode": "auto", "colorMode": "value", "graphMode": "area", "justifyMode": "auto" }
    },
    {
      "title": "Workflows Failed (Total)",
      "type": "stat",
      "datasource": "Prometheus",
      "id": 5,
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 1 },
      "targets": [
        {
          "expr": "sum(increase(payment_workflows_failed_permanently_total[$__range]))",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "short", "mappings": [], "thresholds": { "mode": "absolute", "steps": [{ "color": "red", "value": 0 }] } },
        "overrides": []
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "orientation": "auto", "textMode": "auto", "colorMode": "value", "graphMode": "area", "justifyMode": "auto" }
    },
