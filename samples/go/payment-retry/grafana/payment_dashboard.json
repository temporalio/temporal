      "fieldConfig": {
        "defaults": { "unit": "percent", "mappings": [], "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": 80 },{ "color": "orange", "value": 60 }, { "color": "red", "value": null }] } },
        "overrides": []
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "orientation": "auto", "textMode": "auto", "colorMode": "value", "graphMode": "area", "justifyMode": "auto" }
    },
    {
      "title": "Simulated Failures (Total)",
      "type": "stat",
      "datasource": "Prometheus",
      "id": 9,
      "gridPos": { "h": 8, "w": 8, "x": 16, "y": 10 },
      "targets": [
        {
          "expr": "sum(increase(payment_activity_failure_simulated_total[$__range]))",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "short", "mappings": [], "thresholds": { "mode": "absolute", "steps": [{ "color": "orange", "value": 0 }] } },
        "overrides": []
      },
      "options": { "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false }, "orientation": "auto", "textMode": "auto", "colorMode": "value", "graphMode": "area", "justifyMode": "auto" }
    },
    {
      "type": "row",
      "title": "Business Metrics",
      "gridPos": { "h": 1, "w": 24, "x": 0, "y": 18 },
      "id": 10,
      "panels": []
    },
    {
      "title": "Total Amount Processed",
      "type": "stat",
      "datasource": "Prometheus",
      "id": 11,
      "gridPos": { "h": 8, "w": 24, "x": 0, "y": 19 },
      "targets": [
