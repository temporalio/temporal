          "refId": "C"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "ops" } }
    },
    {
      "title": "Activity Rates",
      "type": "timeseries",
      "datasource": "Prometheus",
      "id": 14,
      "gridPos": { "h": 9, "w": 12, "x": 12, "y": 28 },
      "targets": [
        {
          "expr": "sum(rate(payment_activity_invocations_total[1m]))",
          "legendFormat": "Invoked",
          "refId": "A"
        },
        {
          "expr": "sum(rate(payment_activity_success_total[1m]))",
          "legendFormat": "Succeeded",
          "refId": "B"
        },
        {
          "expr": "sum(rate(payment_activity_failure_simulated_total[1m]))",
          "legendFormat": "Simulated Failures",
          "refId": "C"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "ops" } }
    }
