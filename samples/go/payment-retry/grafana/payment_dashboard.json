      "title": "Activity Success Rate",
      "type": "stat",
      "datasource": "Prometheus",
      "id": 8,
      "gridPos": { "h": 8, "w": 8, "x": 8, "y": 10 },
      "targets": [
        {
          "expr": "sum(rate(payment_activity_success_total[5m])) / sum(rate(payment_activity_invocations_total[5m])) * 100",
          "legendFormat": "",
          "refId": "A"
        }
